[access]
	inheritFrom = All-Projects
[project]
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group TWRP Maintainers
	label-Verified = -1..+1 group TWRP Maintainers
	submit = group TWRP Maintainers
	push = group Registered Users
	push = +force group TWRP Maintainers
	owner = group TWRP Maintainers
	pushMerge = group TWRP Maintainers
	create = group TWRP Maintainers
	addPatchSet = group TWRP Maintainers
[access "refs/for/refs/heads/*"]
	owner = group TWRP Maintainers
	pushMerge = group TWRP Maintainers
	abandon = group TWRP Maintainers
	addPatchSet = group TWRP Maintainers
	label-Code-Review = -2..+2 group TWRP Maintainers
	label-Verified = -1..+1 group TWRP Maintainers
	submit = group TWRP Maintainers
	editAssignee = group TWRP Maintainers
