			</button>

			<button>
				<condition var1="tw_navbar_button_position" op="=" var2="1"/>
				<placement x="%home_button_x_1%" y="%navbar_btn_y%" placement="4"/>
				<image resource="home"/>
				<condition var1="tw_busy" var2="0"/>
