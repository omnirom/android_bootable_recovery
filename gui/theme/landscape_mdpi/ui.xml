			<button>
				<condition var1="tw_navbar_button_position" op="=" var2="0"/>
				<condition var1="tw_samsung_navbar" op="=" var2="1"/>
				<placement x="%console_button_x%" y="%navbar_btn_y%" placement="4"/>
				<image resource="back"/>
				<condition var1="tw_busy" var2="0"/>
