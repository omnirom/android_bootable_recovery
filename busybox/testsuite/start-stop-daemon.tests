. ./testing.sh

# testing "test name" "cmd" "expected result" "file input" "stdin"

testing "start-stop-daemon -x without -a" \
	'start-stop-daemon -S -x true 2>&1; echo $?' \
	"0\n" \
	"" ""

testing "start-stop-daemon -a without -x" \
	'start-stop-daemon -S -a false 2>&1; echo $?' \
	"1\n" \
	"" ""

exit $FAILCOUNT
